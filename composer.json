{
  "name": "dnadesign/silverstripe-http-cache",
  "description": "Adds control to Pages to set Expires http headers to help control cache length for browsers, WAFs",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "silverstripe/framework": "^4.0 || ^5.0"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  },
  "autoload": {
    "psr-4": {
      "DNADesign\\HTTPCacheControl\\": "extensions"
    }
  }
}
